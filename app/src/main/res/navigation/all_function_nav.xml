<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/all_function_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.nonameapp.ui.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/home_to_question"
            app:destination="@id/questionFragment" />
        <action
            android:id="@+id/home_to_search"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/home_to_setting"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/home_to_exerciseTest"
            app:destination="@id/exerciseTestFragment" />
    </fragment>

    <fragment
        android:id="@+id/questionFragment"
        android:name="com.example.nonameapp.ui.QuestionFragment"
        android:label="QuestionFragment">
        <action
            android:id="@+id/question_to_home"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.nonameapp.ui.TestFragment"
        android:label="TestFragment" >
        <action
            android:id="@+id/test_to_result"
            app:destination="@id/resultTestFragment" />
        <action
            android:id="@+id/test_to_exercise"
            app:destination="@id/exerciseTestFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultTestFragment"
        android:name="com.example.nonameapp.ui.ResultTestFragment"
        android:label="fragment_result_test"
        tools:layout="@layout/fragment_result_test" >
        <action
            android:id="@+id/result_to_exercise"
            app:destination="@id/exerciseTestFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.nonameapp.ui.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search">
        <action
            android:id="@+id/search_to_home"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.nonameapp.ui.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting">
        <action
            android:id="@+id/setting_to_profile"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/setting_to_home"
            app:destination="@id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.nonameapp.ui.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/profile_to_edit"
            app:destination="@id/editProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.nonameapp.ui.EditProfileFragment"
        android:label="EditProfileFragment">
        <action
            android:id="@+id/edit_to_profile"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/exerciseTestFragment"
        android:name="com.example.nonameapp.ui.ExerciseTestFragment"
        android:label="fragment_exercise_test"
        tools:layout="@layout/fragment_exercise_test" >
        <action
            android:id="@+id/exercise_to_test"
            app:destination="@id/testFragment" />
    </fragment>
</navigation>
